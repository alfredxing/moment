{
	"name": "Moment",
	"version": "1.1.0",
	"manifest_version": 2,
	"description": "Minimalist time and weather on the new tab page.",
	"icons": {
		"16": "icon.16.png",
		"48": "icon.48.png",
		"128": "icon.png"
	},
	"permissions": [ "storage", "unlimitedStorage", "https://api.aerisapi.com/", "geolocation" ],
	"content_security_policy": "script-src 'self' https://api.aerisapi.com/; object-src 'self'",
	"options_page": "options.html",
	"chrome_url_overrides": {
		"newtab": "index.html"
	}
}